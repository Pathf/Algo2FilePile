template < typename T = char >
class AnneauCar{
	private:
		struct Maillon{
			T elt;
			Maillon* prec;
			Maillon* suiv;
		};
		Maillon* tete;
		int nbelt;
	public:
		AnneauCar();
		~AnneauCar();
		bool estVide();
		void ajoute(T c);
		void supprime();
		T courant();
		void avance();
		void recule();
		string compacte();
};

#include "AnneauCar.tpp"
